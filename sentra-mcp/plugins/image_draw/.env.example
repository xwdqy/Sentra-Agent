# 是否启用插件
# type: boolean
# options: true | false
PLUGIN_ENABLED=true

# 插件超时时间（毫秒）
# type: number
# range: 60000-300000
PLUGIN_TIMEOUT_MS=150000

# 绘图模型
# type: string
DRAW_MODEL=luminaArchitect-v1-plus

# 绘图服务地址
# type: string
DRAW_BASE_URL=https://yuanplus.chat/v1

# 绘图服务密钥
# type: string
DRAW_API_KEY=sk-REPLACE_ME

# 调用模式：
# type: enum
# options: chat | images
DRAW_MODE=chat

# 当 DRAW_MODE=images 时生效的图片尺寸（OpenAI 兼容尺寸，如 512x512 / 1024x1024）
# type: string
DRAW_IMAGE_SIZE=1536x1024
