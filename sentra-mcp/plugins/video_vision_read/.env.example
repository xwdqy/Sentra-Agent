# 是否启用插件
# type: boolean
# options: true | false
PLUGIN_ENABLED=true

# 插件超时时间（毫秒）
# type: number
# range: 60000-900000
PLUGIN_TIMEOUT_MS=480000

# 视频分析工具的 API 基础地址（支持 OpenAI 兼容接口）
# type: string
VIDEO_VISION_BASE_URL=https://yuanplus.chat/v1

# 视频分析工具的 API 密钥
# type: string
VIDEO_VISION_API_KEY=sk-REPLACE_ME

# 视频分析工具使用的 LLM 模型（需支持视频分析）
# type: string
VIDEO_VISION_MODEL=qwen3-vl

# 单个视频最大大小限制（MB），默认 50MB
# type: number
# range: 1-5000
VIDEO_VISION_MAX_SIZE_MB=50

# 视频处理超时时间（毫秒），建议设置较大值
# type: number
# range: 60000-900000
PLUGIN_TIMEOUT_MS=360000