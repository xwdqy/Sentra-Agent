# 是否启用插件
# type: boolean
# options: true | false
PLUGIN_ENABLED=true

# 插件超时时间（毫秒）
# type: number
# range: 60000-300000
PLUGIN_TIMEOUT_MS=150000

# 实时搜索工具的 API 基础地址
# type: string
REALTIME_SEARCH_BASE_URL=https://yuanplus.chat/v1

# 实时搜索工具的 API 密钥
# type: string
REALTIME_SEARCH_API_KEY=sk-

# 实时搜索工具使用的 LLM 模型
# type: string
REALTIME_SEARCH_MODEL=gemini-2.5-flash-search

# 实时搜索工具的内部请求超时时间（毫秒）
# type: number
# range: 10000-300000
timeoutMs=90000