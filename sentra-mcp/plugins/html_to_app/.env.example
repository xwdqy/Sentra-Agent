# 是否启用插件
# type: boolean
# options: true | false
PLUGIN_ENABLED=true

# 插件超时时间（毫秒）- 默认执行一次完整打包流程的超时
# type: number
# range: 600000-3600000
PLUGIN_TIMEOUT_MS=1200000

# HTML 到桌面应用工具的 API 基础 URL（默认使用全局配置）
# type: string
HTML_TO_APP_BASE_URL=https://yuanplus.chat/v1

# HTML 到桌面应用工具的 API 密钥
# type: string
HTML_TO_APP_API_KEY=

# HTML 到桌面应用工具使用的 LLM 模型
# type: string
HTML_TO_APP_MODEL=gpt-5.2-codex-max

# 默认使用的应用框架
# type: enum
# options: electron-vanilla | electron-react | electron-vue
HTML_TO_APP_DEFAULT_FRAMEWORK=electron-vanilla

# LLM 输出格式，默认 xml：要求模型输出一个完整的 <sentra_project> XML（更稳定、支持断流续传）
# type: enum
# options: xml | markdown
HTML_TO_APP_OUTPUT_FORMAT=xml

# 输入 html_content 的最大 token 数（可选）。
# 说明：仅用于裁剪传入的 html_content，避免 prompt 过长。未设置/<=0 表示不裁剪。
# type: number
HTML_TO_APP_MAX_INPUT_TOKENS=4096

# token 统计使用的模型（可选）。未设置则使用 cl100k_base。
# type: string
HTML_TO_APP_TOKENIZER_MODEL=

# 生成内容过长被截断时的最大 continue 调用次数
# type: number
HTML_TO_APP_MAX_CONTINUE_CALLS=4

# 输出目录基准路径（相对于项目根目录）
# type: string
HTML_TO_APP_OUTPUT_DIR=artifacts/apps

# 使用的包管理器
# type: enum
# options: npm | pnpm | cnpm | yarn
HTML_TO_APP_PACKAGE_MANAGER=cnpm

# 包管理器安装命令参数（可选）
# type: string
HTML_TO_APP_INSTALL_ARGS=

# Electron 下载镜像地址（解决下载超时问题）
# type: string
HTML_TO_APP_ELECTRON_MIRROR=https://npmmirror.com/mirrors/electron/
HTML_TO_APP_ELECTRON_BUILDER_BINARIES_MIRROR=https://npmmirror.com/mirrors/electron-builder-binaries/

# HTTP/HTTPS 代理设置（可选）
# type: string
HTML_TO_APP_HTTP_PROXY=
HTML_TO_APP_HTTPS_PROXY=

# 是否自动安装依赖（需要本地 Node.js 环境）
# type: boolean
# options: true | false
HTML_TO_APP_AUTO_INSTALL=true

# 是否自动打包为可执行文件（需要 electron-builder）
# type: boolean
# options: true | false
HTML_TO_APP_AUTO_BUILD=true

# 是否压缩打包结果为 zip
# type: boolean
# options: true | false
HTML_TO_APP_AUTO_ZIP=true

# 打包完成后是否删除临时构建文件（保留 zip）
# type: boolean
# options: true | false
HTML_TO_APP_CLEAN_BUILD=true