# 是否启用插件
# type: boolean
# options: true | false
PLUGIN_ENABLED=true

# 插件超时时间（毫秒）
# type: number
# range: 60000-600000
PLUGIN_TIMEOUT_MS=600000

# 基础输出目录（默认 artifacts，相对路径基于此解析）
# type: string
WRITE_FILE_BASE_DIR=artifacts

# 字体文件目录（相对于插件目录，默认 fonts）
# type: string
WRITE_FILE_FONT_DIR=fonts

# 字体文件名
# type: string
WRITE_FILE_FONT_NAME=AaRiFuYiRiKeAiYuan-2.ttf