# PPT 生成插件配置示例

# 是否启用插件（留空或 true 表示启用）
# type: boolean
# options: true | false
#PLUGIN_ENABLED=true

# 用于 ai_generate 模式下生成每页内容的模型配置
# type: string
#PPT_GEN_MODEL=gpt-4o-mini

# LLM 接口基础地址
# type: string
#PPT_GEN_BASE_URL=https://yuanplus.chat/v1

# LLM API 密钥
# type: string
#PPT_GEN_API_KEY=sk-...

# 默认主题
# type: string
#PPT_GEN_DEFAULT_THEME=default
