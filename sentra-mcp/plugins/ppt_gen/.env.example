# 是否启用插件
# type: boolean
# options: true | false
PLUGIN_ENABLED=true

# 插件超时时间（毫秒）
# type: number
# range: 60000-600000
PLUGIN_TIMEOUT_MS=600000

# 用于 ai_generate 模式下生成每页内容的模型配置
# type: string
PPT_GEN_MODEL=gemini-3-flash-preview

# LLM 接口基础地址
# type: string
PPT_GEN_BASE_URL=https://yuanplus.chat/v1

# LLM API 密钥
# type: string
PPT_GEN_API_KEY=sk-...

# 默认主题
# type: string
PPT_GEN_DEFAULT_THEME=default