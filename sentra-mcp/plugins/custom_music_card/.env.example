# 是否启用插件
# type: boolean
# options: true | false
PLUGIN_ENABLED=true

# 插件超时时间（毫秒）
# type: number
# range: 30000-600000
PLUGIN_TIMEOUT_MS=90000

# WebSocket SDK 地址（用于发送消息）
# type: string
WS_SDK_URL=ws://127.0.0.1:6702

# WebSocket 超时时间（毫秒）
# type: number
# range: 5000-60000
WS_SDK_TIMEOUT_MS=15000

# 发送方式（pair 或 object）
# type: enum
# options: pair | object
WS_SDK_ARG_STYLE=pair

# 自定义发送路径（默认使用下面的私聊/群聊路径）
# type: string
WS_SDK_SEND_PATH=

# 私聊发送路径
# type: string
WS_SDK_SEND_PATH_PRIVATE=send.private

# 群聊发送路径
# type: string
WS_SDK_SEND_PATH_GROUP=send.group

# 默认封面图片链接（当未提供 cover_url 参数时使用）
# type: string
DEFAULT_COVER_URL=https://filesystem.site/cdn/20251003/lSl1Vi7WNkZzBm6lNJ3LxvxkaHJ77M.png
