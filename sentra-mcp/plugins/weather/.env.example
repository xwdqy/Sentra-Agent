# 是否启用插件
# type: boolean
# options: true | false
PLUGIN_ENABLED=true

# 插件超时时间（毫秒）
# type: number
# range: 5000-600000
PLUGIN_TIMEOUT_MS=45000

# 和风天气 QWeather API 密钥（必填），在 https://dev.qweather.com/ 申请
# type: string
WEATHER_API_KEY=

# 和风天气 API 域名（可选：devapi.qweather.com=免费，api.qweather.com=付费）
# type: string
# options: devapi.qweather.com | api.qweather.com
WEATHER_API_HOST=devapi.qweather.com

# 默认语言（如 zh、en、jp、kr 等）
# type: string
WEATHER_LANG=zh

# 单位制（m=公制，i=英制）
# type: enum
# options: m | i
WEATHER_UNIT=m
