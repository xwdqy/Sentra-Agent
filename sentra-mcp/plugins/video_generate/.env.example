# 是否启用插件
# type: boolean
# options: true | false
PLUGIN_ENABLED=true

# 插件超时时间（毫秒）
# type: number
# range: 60000-900000
PLUGIN_TIMEOUT_MS=600000

# 视频生成工具的 API 密钥（可留空，使用全局 LLM 配置）
# type: string
VIDEO_API_KEY=

# 视频生成工具的 API 基础地址
# type: string
VIDEO_BASE_URL=https://yuanplus.chat/v1

# 视频生成工具使用的 LLM 模型
# type: string
VIDEO_MODEL=sora-2