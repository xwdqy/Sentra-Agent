# 是否启用插件
# type: boolean
# options: true | false
PLUGIN_ENABLED=true

# 插件超时时间（毫秒）
# type: number
# range: 10000-300000
PLUGIN_TIMEOUT_MS=60000

# WebSocket SDK 通讯地址
# type: string
WS_SDK_URL=ws://localhost:6702

# ws 超时时间（毫秒）
# type: number
# range: 1000-120000
WS_SDK_TIMEOUT_MS=60000

# 每次戳一戳之间的间隔时间（毫秒，默认 300ms）
# type: number
# range: 50-10000
POKE_INTERVAL_MS=300

# 是否使用随机间隔
# type: boolean
# options: true | false
POKE_RANDOM_INTERVAL=true

# 随机间隔的额外范围（毫秒，默认 200，实际间隔 = POKE_INTERVAL_MS + random(0, POKE_RANDOM_RANGE_MS)）
# type: number
# range: 0-60000
POKE_RANDOM_RANGE_MS=200

# 单次戳一戳失败时是否重试（true/false，默认 false）
# type: boolean
# options: true | false
POKE_RETRY_ON_FAILURE=false

# 最大重试次数（默认 1）
# type: number
# range: 0-10
POKE_MAX_RETRIES=1
