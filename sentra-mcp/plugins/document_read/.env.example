# 是否启用插件
# type: boolean
# options: true | false
PLUGIN_ENABLED=true

# 插件超时时间（毫秒）
# type: number
# range: 30000-600000
PLUGIN_TIMEOUT_MS=90000

# 最大文档大小（MB）
# type: number
# range: 1-500
DOC_MAX_FILE_SIZE_MB=18

# 默认编码（可选，留空自动检测），支持: utf8, gbk, gb2312, shift_jis, big5 等
# type: string
# example: utf8
#DOC_DEFAULT_ENCODING=utf8