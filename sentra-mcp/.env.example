# OPENAI_BASE_URL：OpenAI 兼容 LLM 基础 URL
OPENAI_BASE_URL=https://yuanplus.chat/v1

# OPENAI_API_KEY：OpenAI 兼容 API Key
OPENAI_API_KEY=sk-REPLACE_ME

# OPENAI_MODEL：默认对话模型
OPENAI_MODEL=gpt-4.1-mini

# OPENAI_TEMPERATURE：采样温度
OPENAI_TEMPERATURE=0.2

# OPENAI_MAX_TOKENS：最大输出 Token（-1 表示不限制）
OPENAI_MAX_TOKENS=-1

# OPENAI_TOOL_CHOICE：原生 tool 调用策略（auto/native/none）
OPENAI_TOOL_CHOICE=auto

# TOOL_STRATEGY：工具调用策略（native/fc/auto）
TOOL_STRATEGY=fc

# FC_BASE_URL：FC 模式使用的 LLM 基础 URL（未配置则回退到 OPENAI_*）
FC_BASE_URL=https://yuanplus.chat/v1

# FC_API_KEY：FC 模式 API Key
FC_API_KEY=sk-REPLACE_ME

# FC_MODEL：FC 模式默认模型
FC_MODEL=gpt-4o-mini

# FC_TEMPERATURE：FC 模式默认温度
FC_TEMPERATURE=0.2

# FC_MAX_TOKENS：FC 模式最大 Token 数（-1 表示不限制）
FC_MAX_TOKENS=-1

# FC_FORMAT：FC 模式输出格式（sentra=ReAct 行协议）
FC_FORMAT=sentra

# FC_PLAN_MAX_RETRIES：规划阶段最大重试次数
FC_PLAN_MAX_RETRIES=3

# FC_ARG_MAX_RETRIES：参数生成/纠错阶段最大重试次数
FC_ARG_MAX_RETRIES=3

# FC_EVAL_MAX_RETRIES：评估阶段最大重试次数
FC_EVAL_MAX_RETRIES=3

# FC_SUMMARY_MAX_RETRIES：最终总结阶段最大重试次数
FC_SUMMARY_MAX_RETRIES=1

# JUDGE_FC_MODEL：Judge 阶段专用 FC 模型（留空回退 FC_MODEL）
JUDGE_FC_MODEL=

# PLAN_FC_MODEL：Plan 阶段专用 FC 模型
PLAN_FC_MODEL=

# ARG_FC_MODEL：Arg 阶段专用 FC 模型
ARG_FC_MODEL=

# EVAL_FC_MODEL：Eval 阶段专用 FC 模型
EVAL_FC_MODEL=

# SUMMARY_FC_MODEL：Summary 阶段专用 FC 模型
SUMMARY_FC_MODEL=

# REFLECTION_FC_MODEL：Reflection 阶段专用 FC 模型
REFLECTION_FC_MODEL=

# FC_PLAN_TEMPERATURE：Plan 阶段采样温度（未设置则用 FC_TEMPERATURE）
FC_PLAN_TEMPERATURE=0.1

# FC_PLAN_TOP_P：Plan 阶段 top_p（可留空）
FC_PLAN_TOP_P=

# FC_EVAL_TEMPERATURE：Eval 阶段采样温度
FC_EVAL_TEMPERATURE=0.1

# FC_EVAL_TOP_P：Eval 阶段 top_p
FC_EVAL_TOP_P=

# FC_SUMMARY_TEMPERATURE：Summary 阶段采样温度
FC_SUMMARY_TEMPERATURE=0.1

# FC_SUMMARY_TOP_P：Summary 阶段 top_p
FC_SUMMARY_TOP_P=

# FC_REFLECTION_TEMPERATURE：Reflection 阶段采样温度
FC_REFLECTION_TEMPERATURE=0.2

# FC_REFLECTION_TOP_P：Reflection 阶段 top_p
FC_REFLECTION_TOP_P=

# FC_REFLECTION_MAX_RETRIES：Reflection 阶段最大重试次数
FC_REFLECTION_MAX_RETRIES=2

# RERANK_ENABLE：是否启用工具候选精排
RERANK_ENABLE=true

# RERANK_BASE_URL：精排所用 LLM 基础 URL
RERANK_BASE_URL=https://yuanplus.chat/v1

# RERANK_API_KEY：精排 LLM API Key
RERANK_API_KEY=sk-REPLACE_ME

# RERANK_CANDIDATE_K：进入精排的候选数量（-1 表示全部）
RERANK_CANDIDATE_K=-1

# RERANK_TOP_N：精排后保留的候选数量（-1 表示不裁剪）
RERANK_TOP_N=-1

# RERANK_MODEL：精排模型名称
RERANK_MODEL=BAAI/bge-reranker-v2-m3

# RERANK_USE_DESC_FALLBACK：缺少 meta 描述时是否回退
RERANK_USE_DESC_FALLBACK=true

# RERANK_TIMEOUT_MS：精排超时时间（毫秒）
RERANK_TIMEOUT_MS=12000

# RERANK_MAX_SUBQUERIES：最大子查询数（-1 或 0 表示不限制）
RERANK_MAX_SUBQUERIES=-1

# RERANK_AGG_ALPHA：精排聚合参数 alpha
RERANK_AGG_ALPHA=0.1

# RERANK_AGG_BETA：精排聚合参数 beta
RERANK_AGG_BETA=0.5

# RERANK_AGG_GAMMA：精排聚合参数 gamma
RERANK_AGG_GAMMA=0.4

# SUMMARIZER_BASE_URL：总结模型基础 URL
SUMMARIZER_BASE_URL=https://yuanplus.chat/v1

# SUMMARIZER_API_KEY：总结模型 API Key
SUMMARIZER_API_KEY=sk-REPLACE_ME

# SUMMARIZER_MODEL：总结阶段模型
SUMMARIZER_MODEL=gpt-4.1-mini

# SUMMARIZER_TEMPERATURE：总结阶段温度
SUMMARIZER_TEMPERATURE=0.1

# JUDGE_BASE_URL：前置判定模型基础 URL
JUDGE_BASE_URL=https://yuanplus.chat/v1

# JUDGE_API_KEY：前置判定模型 API Key
JUDGE_API_KEY=sk-REPLACE_ME

# JUDGE_MODEL：前置判定模型名称
JUDGE_MODEL=gpt-5-mini

# JUDGE_TEMPERATURE：前置判定温度
JUDGE_TEMPERATURE=0.1

# JUDGE_MAX_TOKENS：前置判定最大 Token（-1 表示不限）
JUDGE_MAX_TOKENS=-1

# JUDGE_RACE_TIMEOUT_MS：判定并发竞速的单次超时时间（毫秒）
JUDGE_RACE_TIMEOUT_MS=12000

# REASONER_BASE_URL：Reasoner 模型基础 URL
REASONER_BASE_URL=https://yuanplus.chat/v1

# REASONER_API_KEY：Reasoner API Key
REASONER_API_KEY=sk-REPLACE_ME

# REASONER_MODEL：Reasoner 模型名称
REASONER_MODEL=gemini-2.5-flash-preview-09-2025

# REASONER_TEMPERATURE：Reasoner 温度
REASONER_TEMPERATURE=0.2

# REASONER_MAX_TOKENS：Reasoner 最大 Token（-1 表示不限）
REASONER_MAX_TOKENS=-1

# PLAN_MAX_STEPS：规划阶段最大步骤数
PLAN_MAX_STEPS=8

# PLAN_MAX_CONCURRENCY：规划阶段最大并发
PLAN_MAX_CONCURRENCY=3

# PLAN_TOTAL_TIME_BUDGET_MS：规划/执行总时间预算（毫秒）
PLAN_TOTAL_TIME_BUDGET_MS=60000

# TOOL_TIMEOUT_MS：单次工具调用超时时间（毫秒）
TOOL_TIMEOUT_MS=70000

# WS_RPC_TIMEOUT_MS：默认 WebSocket SDK 调用超时时间（毫秒）
WS_RPC_TIMEOUT_MS=120000

# TOOL_COOLDOWN_DEFAULT_MS：工具默认冷却时间（毫秒）
TOOL_COOLDOWN_DEFAULT_MS=

# TOOL_COOLDOWN_FUNC_RETRY：函数内部冷却重试次数（建议 0）
TOOL_COOLDOWN_FUNC_RETRY=0

# TOOL_CACHE_ENABLE：是否启用工具结果缓存（仅缓存成功结果）
TOOL_CACHE_ENABLE=false

# TOOL_CACHE_TTL_SEC：工具缓存 TTL（秒）
TOOL_CACHE_TTL_SEC=600

# TOOL_CACHE_ALLOWLIST：允许缓存的工具列表（逗号分隔，空=全部）
TOOL_CACHE_ALLOWLIST=

# TOOL_CACHE_DENYLIST：禁止缓存的工具列表（逗号分隔，优先生效）
TOOL_CACHE_DENYLIST=

# PLANNER_TOOL_CONCURRENCY_DEFAULT：单工具默认并发上限
PLANNER_TOOL_CONCURRENCY_DEFAULT=5

# PLANNER_PROVIDER_CONCURRENCY_DEFAULT：单提供方默认并发上限
PLANNER_PROVIDER_CONCURRENCY_DEFAULT=10

# DEFAULT_SCOPE：治理默认 Scope
DEFAULT_SCOPE=global

# DEFAULT_TENANT_ID：治理默认租户 ID
DEFAULT_TENANT_ID=default

# REDIS_HOST：Redis 主机地址
REDIS_HOST=127.0.0.1

# REDIS_PORT：Redis 端口
REDIS_PORT=6379

# REDIS_DB：Redis 数据库编号
REDIS_DB=0

# REDIS_PASSWORD：Redis 密码
REDIS_PASSWORD=

# REDIS_METRICS_PREFIX：Redis 指标前缀
REDIS_METRICS_PREFIX=sentra:mcp:metrics

# REDIS_CONTEXT_PREFIX：Redis 上下文前缀
REDIS_CONTEXT_PREFIX=sentra:mcp:ctx

# MCP_SERVER_TRANSPORT：MCP Server 传输方式（stdio/http 等）
MCP_SERVER_TRANSPORT=stdio

# HTTP_PORT：MCP HTTP Server 端口
HTTP_PORT=3001

# ALLOWED_HOSTS：允许的主机列表（逗号分隔）
ALLOWED_HOSTS=127.0.0.1,localhost

# RUN_ENABLE_REPAIR：是否开启全局修复/回滚重试
RUN_ENABLE_REPAIR=false

# RUN_MAX_REPAIRS：全局修复循环上限次数
RUN_MAX_REPAIRS=0

# RETRY_DIVERSIFY：重试时是否强制多样化路径
RETRY_DIVERSIFY=true
