# range: 0-10
MAX_RETRIES=3

# 请求超时时间（毫秒）
# type: number
# range: 10000-300000
TIMEOUT=120000

# 主动 Planner 专用模型 - 用于生成主动发言计划
# type: string
PROACTIVE_DIRECTIVE_MODEL=gemini-flash-latest

# 主动 Planner 使用的温度 - 控制生成计划的创造性
# type: number
# range: 0.0-2.0
PROACTIVE_DIRECTIVE_TEMPERATURE=1

# 主动 Planner 最大 token 数 - -1 表示不限制
# type: number
# range: -1-32000
PROACTIVE_DIRECTIVE_MAX_TOKENS=-1

# 主动 Planner 超时时间（毫秒）
# type: number
# range: 10000-120000
PROACTIVE_DIRECTIVE_TIMEOUT=60000

# 主动 Planner 重试次数
# type: number
# range: 0-5
