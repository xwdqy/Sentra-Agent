# range: 0-50
AGENT_PRESET_TEACHING_CONTEXT_EXAMPLES=8

# OpenAI 兼容 API 基础地址 - 支持各类兼容服务
# type: string
API_BASE_URL=https://yuanplus.chat/v1

# API 密钥 - 用于身份验证
# type: string
API_KEY=your_api_key_here

# 主要 AI 模型 - 用于所有对话场景
# type: string
MAIN_AI_MODEL=gemini-2.5-pro

# Temperature - 控制回复随机性（建议 0.5-0.8）
# type: number
# range: 0.0-2.0
